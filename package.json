{
  "name": "trying-electron-js",
  "version": "1.0.0",
  "description": "educational purposes",
  "main": "./app/app.js",
  "scripts": {
    "start": "npm test && webpack && electron .",
    "build": "webpack",
    "test": "mocha --compilers js:babel-register --recursive",
    "packager": "electron-packager",
    "packTo:linux": "electron-packager ./ --platform=linux --arch=x64",
    "packTo:mac": "electron-packager ./ --platform=mas --arch=x64",
    "packTo:win": "electron-packager ./ --platform=win32 --arch=x64"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jberivera/trying-electron-js.git"
  },
  "keywords": [
    "boilerplate",
    "electron"
  ],
  "author": "jberivera",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jberivera/trying-electron-js/issues"
  },
  "homepage": "https://github.com/Jberivera/trying-electron-js#readme",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "css-loader": "^0.26.1",
    "electron-packager": "^8.5.1",
    "electron-prebuilt": "^1.4.13",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.3.0",
    "sass-loader": "^5.0.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-merge": "^2.6.1"
  }
}
